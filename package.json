{
    "name": "rce-engine-worker",
    "version": "1.0.0",
    "description": "This worker is a RabbitMQ consumer and will run any code given to it and save the result to the database",
    "main": "dist/src/index.js",
    "scripts": {
        "watch": "npm run clean && tsc && npm run copy && tsc --watch",
        "clean": "rm -rf ./dist",
        "build": "npm run clean && tsc && npm run copy",
        "copy": "cp -R ./src/core/config ./dist/src/core/config"
    },
    "prisma": {
        "schema": "core/db/schema.prisma"
    },
    "keywords": [],
    "author": "FusionSid",
    "license": "MIT",
    "dependencies": {
        "@prisma/client": "^3.13.0",
        "amqplib": "^0.10.3",
        "redis": "^4.6.7",
        "tmp-promise": "^3.0.3",
        "uuid": "^9.0.0",
        "winston": "^3.10.0",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@types/amqplib": "^0.10.1",
        "@types/uuid": "^9.0.2",
        "copyfiles": "^2.4.1",
        "prisma": "^5.1.1"
    }
}
