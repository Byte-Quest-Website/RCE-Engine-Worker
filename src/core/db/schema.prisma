datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Job {
  id          String    @id @unique @default(uuid())
  completed   Boolean   @default(false)
  createdAt   DateTime  @default(now())
  completedAt DateTime?
  success     Boolean?
  report      Json?
}
